{
  "name": "fusion-x",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint-staged": "npx lint-staged",
    "format": "prettier --write src/",
    "prepare": "husky install",
    "generate-css-variables": "npx ts-node --esm --project tsconfig.node.json ./scripts/generate-css-variables.ts",
    "gen-efk-icons": "npx ts-node --esm --project tsconfig.node.json ./scripts/generate-efk-svg-comp.ts"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,vue}": [
      "eslint --fix",
      "prettier --write src/"
    ]
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@codemirror/lang-javascript": "^6.2.2",
    "@codemirror/state": "^6.4.1",
    "@vueuse/core": "^10.11.1",
    "@vueuse/integrations": "^11.0.3",
    "@wangeditor/editor": "^5.1.23",
    "@wangeditor/editor-for-vue": "^5.1.12",
    "ant-design-vue": "^4.2.4",
    "axios": "^1.7.7",
    "codejar": "^4.2.0",
    "codemirror": "^6.0.1",
    "crypto-js": "^4.2.0",
    "custom-vue-scrollbar": "^0.0.8",
    "dayjs": "^1.11.13",
    "echarts": "^5.5.1",
    "file-saver": "^2.0.5",
    "jsencrypt": "^3.3.2",
    "lodash": "^4.17.21",
    "nanoid": "^5.0.7",
    "nprogress": "^0.2.0",
    "pinia": "^2.2.2",
    "qs": "^6.13.0",
    "safe-eval": "^0.4.1",
    "shiki": "^1.17.6",
    "vue": "^3.5.5",
    "vue-codemirror": "^6.1.1",
    "vue-echarts": "^6.7.3",
    "vue-i18n": "^9.14.0",
    "vue-router": "^4.4.5",
    "vuedraggable": "^4.1.0",
    "web-storage-cache": "^1.1.1"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.4",
    "@tsconfig/node18": "^18.2.4",
    "@types/crypto-js": "^4.2.2",
    "@types/file-saver": "^2.0.7",
    "@types/lodash": "^4.17.7",
    "@types/node": "^18.19.50",
    "@types/nprogress": "^0.2.3",
    "@types/qs": "^6.9.16",
    "@unocss/transformer-directives": "^0.58.9",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.28.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.10",
    "npm-run-all2": "^6.2.3",
    "prettier": "^3.3.3",
    "sass": "^1.78.0",
    "ts-node": "^10.9.2",
    "type-fest": "^4.26.1",
    "typescript": "~5.3.3",
    "unocss": "^0.58.9",
    "vite": "^5.4.5",
    "vue-tsc": "^1.8.27"
  }
}
