{
  "name": "fusionx-admin",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "format": "prettier --write src/",
    "prepare": "husky",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint-staged": "npx lint-staged",
    "lint:eslint": "eslint . --fix",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint": "run-s lint:*",
    "cmd:gen-css-vars": "tsx ./scripts/gen-css-vars.ts",
    "cmd:gen-fux-icons": "npx ts-node --esm --project tsconfig.node.json ./scripts/gen-icons.ts",
    "cmd:gen-icon-dts": "npx ts-node --esm --project tsconfig.node.json ./scripts/gen-icon-dts.ts"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,vue}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@codemirror/lang-javascript": "^6.2.2",
    "@codemirror/state": "^6.5.0",
    "@fusionx/utils": "^1.3.2",
    "@vueuse/core": "^10.11.1",
    "@vueuse/integrations": "^11.3.0",
    "@vueuse/math": "^11.3.0",
    "@wangeditor/editor": "^5.1.23",
    "@wangeditor/editor-for-vue": "^5.1.12",
    "ant-design-vue": "^4.2.6",
    "axios": "^1.7.9",
    "codemirror": "^6.0.1",
    "crypto-js": "^4.2.0",
    "custom-vue-scrollbar": "^0.0.8",
    "dayjs": "^1.11.13",
    "echarts": "^5.5.1",
    "file-saver": "^2.0.5",
    "jsencrypt": "^3.3.2",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.3.0",
    "qs": "^6.13.1",
    "safer-eval": "^1.3.6",
    "shiki": "^1.24.2",
    "vue": "^3.5.13",
    "vue-codemirror": "^6.1.1",
    "vue-countup-v3": "^1.4.2",
    "vue-echarts": "^6.7.3",
    "vue-router": "^4.5.0",
    "vuedraggable": "^4.1.0",
    "web-storage-cache": "^1.1.1"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.4",
    "@types/crypto-js": "^4.2.2",
    "@types/file-saver": "^2.0.7",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.10.2",
    "@types/nprogress": "^0.2.3",
    "@types/qs": "^6.9.17",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.4",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^9.17.0",
    "eslint-plugin-oxlint": "^0.15.2",
    "eslint-plugin-vue": "^9.32.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "npm-run-all2": "^7.0.1",
    "prettier": "^3.4.2",
    "sass": "^1.83.0",
    "tsx": "^4.19.2",
    "type-fest": "^4.30.1",
    "typescript": "~5.3.3",
    "unocss": "^0.63.6",
    "unplugin-auto-import": "^0.18.6",
    "vite": "^5.4.11",
    "vite-plugin-vue-devtools": "^7.6.8",
    "vue-tsc": "^2.1.10"
  }
}
