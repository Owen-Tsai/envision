{
  "name": "fusion-x",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --fix",
    "lint:staged": "npx lint-staged",
    "format": "prettier --write src/",
    "prepare": "husky install",
    "cmd:gen-css-vars": "npx ts-node --esm --project tsconfig.node.json ./scripts/gen-css-vars.ts",
    "cmd:gen-fux-icons": "npx ts-node --esm --project tsconfig.node.json ./scripts/gen-icons.ts",
    "cmd:gen-icon-dts": "npx ts-node --esm --project tsconfig.node.json ./scripts/gen-icon-dts.ts"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,vue}": [
      "eslint --fix",
      "prettier --write src/"
    ]
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@codemirror/lang-javascript": "^6.2.2",
    "@codemirror/state": "^6.4.1",
    "@fusionx/utils": "^1.3.2",
    "@vueuse/core": "^10.11.1",
    "@vueuse/integrations": "^11.1.0",
    "@vueuse/math": "^11.1.0",
    "@wangeditor/editor": "^5.1.23",
    "@wangeditor/editor-for-vue": "^5.1.12",
    "ant-design-vue": "^4.2.5",
    "axios": "^1.7.7",
    "codejar": "^4.2.0",
    "codemirror": "^6.0.1",
    "crypto-js": "^4.2.0",
    "custom-vue-scrollbar": "^0.0.8",
    "dayjs": "^1.11.13",
    "echarts": "^5.5.1",
    "file-saver": "^2.0.5",
    "jsencrypt": "^3.3.2",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.1",
    "nanoid": "^5.0.7",
    "nprogress": "^0.2.0",
    "pinia": "^2.2.4",
    "qs": "^6.13.0",
    "safer-eval": "^1.3.6",
    "shiki": "^1.22.0",
    "vue": "^3.5.12",
    "vue-codemirror": "^6.1.1",
    "vue-countup-v3": "^1.4.2",
    "vue-echarts": "^6.7.3",
    "vue-i18n": "^9.14.1",
    "vue-router": "^4.4.5",
    "vuedraggable": "^4.1.0",
    "web-storage-cache": "^1.1.1"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.4",
    "@tsconfig/node20": "^20.1.4",
    "@types/crypto-js": "^4.2.2",
    "@types/file-saver": "^2.0.7",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.7.5",
    "@types/nprogress": "^0.2.3",
    "@types/qs": "^6.9.16",
    "@unocss/transformer-directives": "^0.58.9",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vue/eslint-config-prettier": "^10.0.0",
    "@vue/eslint-config-typescript": "^14.1.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^9.12.0",
    "eslint-plugin-vue": "^9.29.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "npm-run-all2": "^6.2.6",
    "prettier": "^3.3.3",
    "sass": "^1.80.3",
    "ts-node": "^10.9.2",
    "type-fest": "^4.26.1",
    "typescript": "~5.3.3",
    "unocss": "^0.63.4",
    "vite": "^5.4.9",
    "vue-tsc": "^2.1.6"
  }
}
