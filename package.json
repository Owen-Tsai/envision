{
  "name": "envision",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint-staged": "npx lint-staged",
    "format": "prettier --write src/",
    "prepare": "husky install",
    "generate-css-variables": "npx ts-node --esm --project tsconfig.node.json ./scripts/generate-css-variables.ts"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,vue}": [
      "eslint --fix",
      "prettier --write src/"
    ]
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@vueuse/core": "^10.7.1",
    "ant-design-vue": "^4.1.0",
    "axios": "^1.6.5",
    "custom-vue-scrollbar": "^0.0.8",
    "echarts": "^5.4.3",
    "file-saver": "^2.0.5",
    "jsencrypt": "^3.3.2",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "qs": "^6.11.2",
    "vue": "^3.3.11",
    "vue-echarts": "^6.6.8",
    "vue-i18n": "9",
    "vue-router": "^4.2.5",
    "web-storage-cache": "^1.1.1"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/file-saver": "^2.0.7",
    "@types/node": "^18.19.3",
    "@types/nprogress": "^0.2.3",
    "@types/qs": "^6.9.11",
    "@unocss/transformer-directives": "^0.58.3",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.0",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.0.3",
    "sass": "^1.69.7",
    "ts-node": "^10.9.2",
    "typescript": "~5.3.0",
    "unocss": "^0.58.3",
    "vite": "^5.0.10",
    "vue-tsc": "^1.8.25"
  }
}
